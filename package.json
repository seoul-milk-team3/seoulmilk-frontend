{
  "name": "seoulmilk-front",
  "private": true,
  "scripts": {
    "lint": "pnpm --recursive eslint .",
    "lint:fix": "pnpm --recursive eslint . --fix",
    "main": "pnpm --filter main",
    "tsconfig": "pnpm --filter @seoulmilk/tsconfig",
    "styles": "pnpm --filter @seoulmilk/styles",
    "ui": "pnpm --filter @seoulmilk/ui",
    "utils": "pnpm --filter @seoulmilk/utils",
    "build:ui": "pnpm --filter @seoulmilk/ui build",
    "build:styles": "pnpm --filter @seoulmilk/styles build",
    "svgr": "pnpm --filter @seoulmilk/icon generate",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "chromatic": "chromatic --exit-zero-on-changes"
  },
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "root": true,
  "packageManager": "pnpm@9.1.3",
  "engines": {
    "node": ">=18",
    "pnpm": ">=9"
  },
  "dependencies": {
    "@emotion/react": "^11.13.3",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "tsup": "^8.3.0"
  },
  "devDependencies": {
    "@types/node": "^22.7.5",
    "@types/react": "^18.3.11",
    "@types/react-dom": "^18.3.0",
    "concurrently": "^9.0.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.6.2",
    "@chromatic-com/storybook": "^3.2.4",
    "@storybook/addon-essentials": "^8.5.8",
    "@storybook/addon-interactions": "^8.5.8",
    "@storybook/addon-onboarding": "^8.5.8",
    "@storybook/blocks": "^8.5.8",
    "@storybook/react": "^8.5.8",
    "@storybook/react-vite": "^8.5.8",
    "@storybook/test": "^8.5.8",
    "storybook": "^8.5.8",
    "storybook-addon-theme-provider": "^0.2.9",
    "@eslint/js": "^9.19.0",
    "@trivago/prettier-plugin-sort-imports": "^5.2.2",
    "eslint": "^9.19.0",
    "eslint-plugin-prettier": "^5.2.3",
    "eslint-plugin-react-hooks": "^5.0.0",
    "eslint-plugin-react-refresh": "^0.4.18",
    "eslint-plugin-unused-imports": "^4.1.4",
    "prettier": "^3.5.2",
    "typescript-eslint": "^8.22.0"
  }
}
